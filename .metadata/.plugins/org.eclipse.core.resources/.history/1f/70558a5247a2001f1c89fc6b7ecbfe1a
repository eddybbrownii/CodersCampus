package CodersCampus;

public class Human {

	String gender;
	String weight;
	String height;
	String eyeColor;

	String getDetails() {

		return "Gender: " + gender + ", height" + height + ", Eye Color: " + eyeColor + ", Weight: " + weight;
	}

	void setProperties(String gender, String weight,
			String height, String eyeColor) {
		this.gender = gender;
		this.height = height;
		this.weight = weight;
		this.eyeColor = eyeColor;
	}
}